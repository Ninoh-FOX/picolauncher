pico-8 cartridge // http://www.pico-8.com
version 42
__lua__

function _menutest()
end

function _init()
    select=0
    carts=ls()
    menuitem(1, 'menutest', _menutest)
end

function _update()
    if btnp(2) then
        select=(select-1)%(#carts)
    end
    if btnp(3) then
        select=(select+1)%(#carts)
    end
    if btnp(5) then
        load(carts[select+1], 'exit')
    end
end

function _draw()
    cls(1)
    print('select game')
    for i, filename in ipairs(carts) do
        c=6
        if select+1 == i then
            c=7
        end
        color(c)
        print(i .. ': ' .. filename)
    end
end


